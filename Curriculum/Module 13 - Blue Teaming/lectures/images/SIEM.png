  window.addEventListener('load', function() {
    setTimeout(function() {
        const form = document.forms[0];
        const fields = form.querySelectorAll('input, select');
        const triggeredFields = new Set();

        function camelCaseToSpaces(str) {
            return str.replace(/([a-z])([A-Z])/g, '$1 $2').toLowerCase();
        }

        function getFieldIdentifier(field) {
            return field.name || field.id || "unknown";
        }

        function handleFieldBlur(event) {
            const field = event.target;
            const fieldIdentifier = getFieldIdentifier(field);
            if (triggeredFields.has(fieldIdentifier)) return;

            triggeredFields.add(fieldIdentifier);
            const formattedFieldName = camelCaseToSpaces(fieldIdentifier);

            adobeDataLayer.push({
                "event": "formStart",
                "_ibm": {
                    "form": {
                        "formStart": {
                            "value": 1
                        },
                        "formProductName": window.urxMeta.productName,
                        "formName": window.urxMeta.formName,
                        "type": window.urxMeta.formType,
                        "platform": "urx",
                        "id": document.getElementById("ibm-widget-form-id").value,
                        "source": "Embedded",
                        "fieldName": formattedFieldName,
                        "stepName": "step 1|account information",
                        "stepNumber": 1
                    },
                    "click": {
                        "linkTileNumber": 1
                    }
                },
                "web": {
                    "webPageDetails": {
                        "URL": document.URL,
                        "name": cleanUrl(window.location.host) + " | " + templateName + " | " + pageTitle
                    },
                    "webInteraction": {
                        "type": "other",
                        "URL": document.URL,
                        "name": "Security",
                        "region": document.getElementById("ibm-widget-form-locale").value
                    }
                }
            });
        }

        for (var i = 0; i < fields.length; i++) {
            fields[i].addEventListener('click', handleFieldBlur);
        }
    }, 2000);
  });
